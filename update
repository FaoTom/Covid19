#!/bin/bash
cd ../COVID-19
git pull origin master
cd ../COV
cp ../COVID-19/dati-andamento-nazionale/dpc-covid19-ita-andamento-nazionale.csv dpc-covid19-ita-andamento-nazionale.csv 
